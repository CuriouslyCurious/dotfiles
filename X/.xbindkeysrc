# Volume control
"amixer set Master 1%+"
    XF86AudioRaiseVolume

"amixer set Master 1%-"
    XF86AudioLowerVolume

"amixer set Master toggle"
    XF86AudioMute

# Brightness control
"xbacklight -inc 3"
    XF86MonBrightnessUp

"xbacklight -dec 3"
    XF86MonBrightnessDown

# Power control
"systemctl hibernate"
    XF86Sleep

