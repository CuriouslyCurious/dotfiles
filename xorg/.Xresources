! To reread your .Xresources file, and throw away your old resources:
! xrdb ~/.Xresources
! To reread your .Xresources file, and keep your old resources:
! xrdb -merge ~/.Xresources

! ----------------
! Xft settings
! ----------------
Xft.antialias:             		true
Xft.autohint:              		false
Xft.hinting:               		true
Xft.hintstyle:             		hintslight
Xft.dpi:                   		96
Xft.rgba:                  		rgb
Xft.lcdfilter:             		lcdlight

! ---------------
! URxvt
! ---------------
URxvt.keysym.Control-Up:        \033[1;5A
URxvt.keysym.Control-Down:      \033[1;5B
URxvt.keysym.Control-Left:      \033[1;5D
URxvt.keysym.Control-Right:     \033[1;5C


URxvt*termName:            		rxvt-unicode-256color
URxvt*loginShell:          		true
URxvt*scrollBar:           		false
URxvt.scrollStyle: 				plain
URxvt.secondaryScroll: 			true
URxvt*cursorBlink:         		true
! URxvt*font:                 	xft:Hack:size=9:antialias=true
! URxvt*font:                 	-*-terminus-medium-r-*-*-12-*-*-*-*-*-iso8859-*
URxvt*font:                     xft:DejaVuSansMono:size=10

URxvt*saveLines:           		4096
! URxvt.perl-ext-common: 			default,clipboard,url-select,keyboard-select
! URxvt.url-select.launcher: 		firefox
URxvt.url-select.underline: 	true
URxvt.url-select.button: 		1
URxvt*colorUL: 					yellow
! URxvt*clipboard.copycmd:   		xsel -ib
! URxvt*clipboard.pastecmd:  		xsel -0b
! URxvt*letterSpace:         		-4

URxvt*urlLLauncher:             firefox

URxvt*transparent:              true
URxvt*shading:                  15

